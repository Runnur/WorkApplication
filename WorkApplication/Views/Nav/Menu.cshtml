@model IEnumerable<string>
@{
    Layout = null;
}

@Html.ActionLink("Домой", "List", "Home", null, new { @class = "btn btn-block btn-default btn-lg" })

@foreach(var link in Model)
{
    @Html.RouteLink(link, new { controller = "Home", action = "List", category = link, page = 1 },
        new { @class = "btn btn-block btn-default btn-lg" })
}